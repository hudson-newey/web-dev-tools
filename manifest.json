{
  "name": "Web Dev Tools",
  "description": "Basic web development tools",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://*/*",
        "http://*/*"
      ]
    }
  ]
}
